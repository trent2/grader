<html t:type="layout" title="${message:title}"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">
	<body>
		<t:beaneditform object="gradeList" t:submitLabel="message:create">
			<t:parameter name="period">
				<t:hibernateselectlist t:id="period" entityClass="PeriodClass" />
			</t:parameter>
			<t:parameter name="pupilGroup">
				<t:hibernateselectlist t:id="pupilGroup" entityClass="PupilGroupClass" />
			</t:parameter>
			<t:parameter name="subject">
				<t:hibernateselectlist t:id="subject" entityClass="SubjectClass" />
			</t:parameter>
			<t:parameter name="teacher">
				<t:hibernateselectlist t:id="teacher" entityClass="TeacherClass" />
			</t:parameter>
		</t:beaneditform>
	</body>
</html>